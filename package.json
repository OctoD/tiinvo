{
  "name": "tiinvo",
  "version": "1.6.3",
  "author": "Paolo Roth <octod>",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "Handling possible errors or values in a functional way.",
  "keywords": [
    "functional programming",
    "functional",
    "octod",
    "option",
    "result",
    "try catch",
    "typescript"
  ],
  "scripts": {
    "benchmarks": "ts-node .src/benchmarks",
    "changelog": "standard-changelog",
    "docs": "typedoc --options typedoc.markdown.json",
    "lint": "eslint -c ./.eslintrc.json src/**/*.ts --fix",
    "prepublishOnly": "tsc -p .",
    "size-limit": "tsc -p . && size-limit",
    "test": "jest",
    "upgrade-interactive": "npm-check --update"
  },
  "homepage": "https://github.com/octod/tiinvo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octod/tiinvo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "size-limit": [
    {
      "limit": "1 s",
      "webpack": false,
      "path": "dist/*.js"
    }
  ],
  "devDependencies": {
    "@types/benchmark": "1.0.31",
    "@types/jest": "25.1.2",
    "@types/node": "13.7.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "benchmark": "2.1.4",
    "codecov": "3.6.5",
    "eslint": "6.8.0",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "lint-staged": "10.0.7",
    "npm-check": "5.9.0",
    "prettier": "1.19.1",
    "size-limit": "4.0.1",
    "standard-changelog": "2.0.21",
    "ts-jest": "25.2.0",
    "ts-node": "8.6.2",
    "typedoc": "0.16.9",
    "typedoc-plugin-markdown": "2.2.16",
    "typescript": "3.7.5"
  }
}
