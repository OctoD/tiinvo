{
  "name": "tiinvo",
  "version": "2.1.0",
  "author": "Paolo Roth <octod>",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "Functional data structures for TypeScript and JavaScript.",
  "types": "./dist/index.d.ts",
  "keywords": [
    "functional programming",
    "functional",
    "octod",
    "option",
    "tagged type",
    "tagged types",
    "pipe",
    "predicate",
    "typeguards",
    "result",
    "try catch",
    "typescript"
  ],
  "scripts": {
    "benchmarks": "ts-node .src/benchmarks",
    "changelog": "standard-changelog",
    "docs": "typedoc --options typedoc.markdown.json",
    "lint": "eslint -c ./.eslintrc.json src/**/*.ts --fix",
    "prepublishOnly": "tsc -p .",
    "size-limit": "tsc -p . && size-limit",
    "test": "jest",
    "upgrade-interactive": "npm-check --update"
  },
  "homepage": "https://github.com/octod/tiinvo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octod/tiinvo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "size-limit": [
    {
      "limit": "1 s",
      "webpack": false,
      "path": "dist/*.js"
    }
  ],
  "devDependencies": {
    "@types/benchmark": "^1.0.33",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "benchmark": "2.1.4",
    "codecov": "^3.7.2",
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "npm-check": "^5.9.2",
    "prettier": "^2.1.1",
    "size-limit": "^4.6.0",
    "standard-changelog": "^2.0.24",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.1",
    "typedoc-plugin-markdown": "^2.4.2",
    "typescript": "^4.0.2"
  }
}
