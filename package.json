{
  "name": "tiinvo",
  "version": "3.0.2",
  "author": "Paolo Roth <octod>",
  "license": "MIT",
  "description": "A lib of types and utilities for your TypeScript and JavaScript projects",
  "types": "./index.d.ts",
  "directories": {
    "lib": "."
  },
  "exports": {
    "./*": "./*",
    "./a": {
      "import": "./array.mjs",
      "require": "./array.js"
    },
    "./b": {
      "import": "./bool.mjs",
      "require": "./bool.js"
    },
    "./d": {
      "import": "./date.mjs",
      "require": "./date.js"
    },
    "./e": {
      "import": "./either.mjs",
      "require": "./either.js"
    },
    "./f": {
      "import": "./functors.mjs",
      "require": "./functors.js"
    },
    "./fn": {
      "import": "./fn.mjs",
      "require": "./fn.js"
    },
    "./fx": {
      "import": "./function.mjs",
      "require": "./function.js"
    },
    "./m": {
      "import": "./maybe.mjs",
      "require": "./maybe.js"
    },
    "./num": {
      "import": "./num.mjs",
      "require": "./num.js"
    },
    "./o": {
      "import": "./option.mjs",
      "require": "./option.js"
    },
    "./obj": {
      "import": "./obj.mjs",
      "require": "./obj.js"
    },
    "./p": {
      "import": "./predicate.mjs",
      "require": "./predicate.js"
    },
    "./pipe": {
      "import": "./pipe.mjs",
      "require": "./pipe.js"
    },
    "./pipe.async": {
      "import": "./pipe.async.mjs",
      "require": "./pipe.async.js"
    },
    "./r": {
      "import": "./result.mjs",
      "require": "./result.js"
    },
    "./str": {
      "import": "./str.mjs",
      "require": "./str.js"
    },
    "./try": {
      "import": "./try.mjs",
      "require": "./try.js"
    },
    "./tuple": {
      "import": "./tuple.mjs",
      "require": "./tuple.js"
    }
  },
  "keywords": [
    "functional programming",
    "functions",
    "utility"
  ],
  "scripts": {
    "build": "tsc -p . && tsc -p ./tsconfig.commonjs.json && node scripts/rename.js && node scripts/copy.js",
    "changelog": "npx standard-changelog --first-release",
    "clean": "tsc --build --clean && node scripts/clean.js",
    "test": "jest",
    "deploy": "npm test && npm run clean && npm run build && cd dist && npm publish && cd .. && npm run clean",
    "postpublish": "npm run clean",
    "upgrade-interactive": "npx npm-check --update"
  },
  "homepage": "https://tiinvo.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octod/tiinvo.git"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "jest": "^27.4.5",
    "standard-changelog": "^2.0.27",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.5"
  }
}
