{
  "name": "tiinvo",
  "version": "2.13.0",
  "author": "Paolo Roth <octod>",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "Lot of functions for tacit programming and functional types for TypeScript and JavaScript.",
  "types": "./dist/index.d.ts",
  "keywords": [
    "functional programming",
    "functional",
    "functional utilities",
    "array functions",
    "octod",
    "option",
    "tagged type",
    "tagged types",
    "pipe",
    "predicate",
    "typeguards",
    "result",
    "try catch",
    "typescript"
  ],
  "scripts": {
    "benchmarks": "ts-node .src/benchmarks",
    "changelog": "standard-changelog",
    "docs": "typedoc --options typedoc.markdown.json",
    "lint": "eslint -c ./.eslintrc.json src/**/*.ts --fix",
    "prepublishOnly": "tsc -p .",
    "test": "jest",
    "upgrade-interactive": "npm-check --update"
  },
  "homepage": "https://github.com/octod/tiinvo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octod/tiinvo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && standard-changelog"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "benchmark": "2.1.4",
    "codecov": "^3.8.1",
    "eslint": "^7.20.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.1",
    "standard-changelog": "^2.0.27",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.25",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.5"
  }
}
