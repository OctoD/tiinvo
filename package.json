{
  "name": "tiinvo",
  "version": "2.17.0",
  "author": "Paolo Roth <octod>",
  "license": "MIT",
  "description": "Lot of functions for tacit programming and functional types for TypeScript and JavaScript.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.ts",
  "esnext": "dist/esm/index.mjs",
  "keywords": [
    "functional programming",
    "tacit programming",
    "functional",
    "tacit",
    "functional utilities",
    "array functions",
    "string functions",
    "object functions",
    "number functions",
    "octod",
    "option",
    "number",
    "string",
    "object",
    "tagged type",
    "tagged types",
    "pipe",
    "predicate",
    "typeguards",
    "result",
    "try catch",
    "typescript"
  ],
  "scripts": {
    "benchmarks": "ts-node .src/benchmarks",
    "changelog": "standard-changelog --first-release",
    "docs:serve": "docsify serve docs -p 12345",
    "lint": "eslint -c ./.eslintrc.json src/**/*.ts --fix",
    "prepublishOnly": "rimraf dist && rollup -c ./rollup.config.js",
    "test": "jest",
    "upgrade-interactive": "npm-check --update"
  },
  "homepage": "https://tiinvo.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octod/tiinvo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "docsify": {
    "config": {
      "basePath": "https://docsify.js.org/",
      "loadSidebar": true,
      "loadNavbar": true,
      "coverpage": true,
      "name": "docsify"
    }
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "benchmark": "2.1.4",
    "codecov": "^3.8.1",
    "docsify-cli": "^4.4.3",
    "eslint": "^7.20.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "standard-changelog": "^2.0.27",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
