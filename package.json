{
  "name": "tiinvo",
  "version": "3.0.0",
  "author": "Paolo Roth <octod>",
  "license": "MIT",
  "description": "A lib of types and utilities for your TypeScript and JavaScript projects",
  "types": "./src/index.d.ts",
  "exports": {
    "./*": "./src/*",
    "./f": "./src/functors.mjs",
    "./m": "./src/maybe.mjs",
    "./o": "./src/option.mjs",
    "./p": "./src/predicate.mjs",
    "./r": "./src/result.mjs"
  },
  "keywords": [
    "functional programming",
    "tacit programming",
    "functional",
    "tacit",
    "functional utilities",
    "array functions",
    "string functions",
    "object functions",
    "number functions",
    "octod",
    "option",
    "number",
    "string",
    "object",
    "tagged type",
    "tagged types",
    "pipe",
    "predicate",
    "typeguards",
    "result",
    "try catch",
    "typescript"
  ],
  "scripts": {
    "benchmarks": "ts-node .src/benchmarks",
    "changelog": "standard-changelog --first-release",
    "docs:serve": "docsify serve docs -p 12345",
    "lint": "eslint -c ./.eslintrc.json src/**/*.ts --fix",
    "prepublishOnly": "rimraf dist && rollup -c ./rollup.config.js",
    "test": "jest",
    "upgrade-interactive": "npm-check --update"
  },
  "homepage": "https://tiinvo.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octod/tiinvo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "docsify": {
    "config": {
      "basePath": "https://docsify.js.org/",
      "loadSidebar": true,
      "loadNavbar": true,
      "coverpage": true,
      "name": "docsify"
    }
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "benchmark": "2.1.4",
    "codecov": "^3.8.1",
    "docsify-cli": "^4.4.3",
    "eslint": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.4",
    "npm-check": "^5.9.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.44.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "standard-changelog": "^2.0.27",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.5"
  }
}
