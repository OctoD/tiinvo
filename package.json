{
  "name": "tiinvo",
  "version": "1.5.1",
  "author": "Paolo Roth <octod>",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "Handling possible errors or values in a functional way.",
  "keywords": [
    "functional programming",
    "functional",
    "octod",
    "option",
    "result",
    "try catch",
    "typescript"
  ],
  "scripts": {
    "benchmarks": "ts-node .src/benchmarks",
    "changelog": "standard-changelog",
    "docs": "typedoc --options typedoc.markdown.json",
    "lint": "eslint -c ./.eslintrc.json src/**/*.ts --fix",
    "prepublishOnly": "tsc -p .",
    "size-limit": "tsc -p . && size-limit",
    "test": "jest",
    "upgrade-interactive": "npm-check --update"
  },
  "homepage": "https://github.com/octod/tiinvo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octod/tiinvo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "size-limit": [
    {
      "limit": "1 s",
      "webpack": false,
      "path": "dist/*.js"
    }
  ],
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.0.12",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "benchmark": "^2.1.4",
    "codecov": "^3.6.1",
    "eslint": "^6.2.2",
    "husky": "^3.0.8",
    "jest": "^24.8.0",
    "lint-staged": "^9.4.2",
    "npm-check": "^5.9.0",
    "prettier": "^1.18.2",
    "size-limit": "^2.1.6",
    "standard-changelog": "^2.0.11",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.10",
    "typescript": "^3.6.2"
  }
}
